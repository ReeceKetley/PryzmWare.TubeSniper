             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Views/Pages/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/FirstFloor.ModernUI;component/Assets/ModernUI.xaml" />
                <ResourceDictionary Source="/Assets/ModernUI.TubeSniper.xaml"/>
                <ResourceDictionary Source="Themes/MyModernWindow.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
